
<div class="container" ng-app ng-init="courses =[
     {number: 'ABCD',name:'.NET BootStrap', instructor:'Sachin'},
     {number: 'EFGH',name:'.NET jquery', instructor:'Sachin'},
     {number: 'IJKL',name:'.NET Angular', instructor:'Sachin'}
     ]">
    <div class="row">
        <ul class="nav navbar-nav">
            <li>@Html.ActionLink("HardCoded", "Index", "Courses")</li>
            <li>@Html.ActionLink("From Code", "FromCode", "Courses")</li>
            <li>@Html.ActionLink("About", "About", "Home")</li>
            <li>@Html.ActionLink("Contact", "Contact", "Home")</li>
        </ul>
    </div>
    <div class="row">
        <div class="span10">
            <table class="table table-condensed table-hover">
                <tr>
                    <th>Course</th>
                    <th>Course Name</th>
                    <th>Instructor</th>
                </tr>
                <tr ng-repeat="course in courses">
                    <td>{{course.number}}</td>
                    <td>{{course.name}}</td>
                    <td>{{course.instructor}}</td>
                </tr>
            </table>  
        </div>
    </div>

</div>